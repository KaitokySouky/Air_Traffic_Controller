<property name="attenteAuSol" column="attenteAuSol" />
<property name="accèsAuPiste" column="accèsAuPiste" />
<property name="délaiAnticollision" column="délaiAnticollision" />
<property name="tempsdécollage" column="tempsdécollage" />
<property name="tempsatterissage" column="tempsatterissage" />
